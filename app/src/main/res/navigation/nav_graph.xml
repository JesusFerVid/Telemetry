<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/nav_graph"
  app:startDestination="@id/VehiculosFragment">

  <fragment
    android:id="@+id/VehiculosFragment"
    android:name="com.jesusfervid.telemetry.view.VehiculosFragment"
    android:label="@string/vehiculos_fragment_label"
    tools:layout="@layout/fragment_vehiculos">

    <action
      android:id="@+id/actionEditarVehiculo"
      app:destination="@id/EditarVehiculoFragment"/>
    <action
      android:id="@+id/actionRevisionesVehiculo"
      app:destination="@id/RevisionesFragment"/>
  </fragment>
  <fragment
    android:id="@+id/EditarVehiculoFragment"
    android:name="com.jesusfervid.telemetry.view.EditarVehiculoFragment"
    android:label="@string/nuevo_vehiculo_fragment_label"
    tools:layout="@layout/fragment_editar_vehiculo" >
    <argument
      android:name="vehiculo"
      app:argType="com.jesusfervid.telemetry.model.Vehiculo"
      app:nullable="true"/>
  </fragment>
  <fragment
    android:id="@+id/RevisionesFragment"
    android:name="com.jesusfervid.telemetry.view.RevisionesFragment"
    android:label="@string/revisiones_fragment_label"
    tools:layout="@layout/fragment_revisiones">
    <argument
      android:name="vehiculo"
      app:argType="com.jesusfervid.telemetry.model.Vehiculo"/>
    <action
      android:id="@+id/actionEditarRevision"
      app:destination="@id/EditarRevisionFragment"/>
  </fragment>
  <fragment
    android:id="@+id/EditarRevisionFragment"
    android:name="com.jesusfervid.telemetry.view.EditarRevisionFragment"
    android:label="@string/nueva_revision_fragment_label"
    tools:layout="@layout/fragment_editar_revision">
    <argument
      android:name="revision"
      app:argType="com.jesusfervid.telemetry.model.Revision"
      app:nullable="true"/>
    <argument
      android:name="vehiculo"
      app:argType="com.jesusfervid.telemetry.model.Vehiculo"
      app:nullable="true"/>
    <action
      android:id="@+id/actionEditarPropiedadesRevision"
      app:destination="@id/EditarPropiedadesRevision"/>
    <action
      android:id="@+id/actionEditarLineaRevision"
      app:destination="@id/EditarLineaRevisionFragment"/>
  </fragment>
  <fragment
    android:id="@+id/EditarPropiedadesRevision"
    android:name="com.jesusfervid.telemetry.view.EditarPropiedadesRevision"
    android:label="@string/editar_propiedades_revision_fragment_label"
    tools:layout="@layout/fragment_editar_propiedades_revision">
    <argument
      android:name="revision"
      app:argType="com.jesusfervid.telemetry.model.Revision"/>
  </fragment>
  <fragment
    android:id="@+id/EditarLineaRevisionFragment"
    android:name="com.jesusfervid.telemetry.view.EditarLineaRevisionFragment"
    android:label="@string/editar_linea_revision_fragment_label"
    tools:layout="@layout/fragment_editar_linea_revision">
    <argument
      android:name="lineaRevision"
      app:argType="com.jesusfervid.telemetry.model.LineaRevision"/>
  </fragment>
</navigation>
